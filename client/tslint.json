{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended",
		"tslint-config-airbnb",
		"tslint-react",
		"tslint-plugin-prettier",
		"tslint-config-prettier"
	],
	"rules": {
    "prettier": true,
		"import-name": false,
		"object-literal-sort-keys": false,
		"jsx-no-lambda": false,
		"jsx-boolean-value": false,
    "interface-name": [true, "never-prefix"],
    "no-console": false,
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    }
	}
}
